{
   "$schema": "node_modules/wrangler/config-schema.json",
   "name": "mdm-barroit-sh",
   "main": "worker/entry.js",
   "compatibility_date": "2025-08-17",
   "assets": {
      "binding": "ASSETS",
      "directory": "./site",
      "html_handling": "drop-trailing-slash",
   },
   "observability": {
      "enabled": true,
   },
   "routes": [
      {
         "pattern": "mdm.barroit.sh",
         "custom_domain": true,
      },
   ],
}
